{
    "manifest_version": 3,
    "name": "Unsigned Keyboard",
    "version": "1.0",
    "description": "Transliterates English text to Assamese as you type",
    "permissions": ["activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }